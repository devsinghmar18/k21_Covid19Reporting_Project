{
	"name": "df_transform_hospital_admissions",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "df_hospital_admission_ds",
						"type": "DatasetReference"
					},
					"name": "HospitalAdmissionSource"
				},
				{
					"dataset": {
						"referenceName": "ds_dim_date_lookup",
						"type": "DatasetReference"
					},
					"name": "DimDataSource"
				},
				{
					"dataset": {
						"referenceName": "df_country_lookup",
						"type": "DatasetReference"
					},
					"name": "CountrySource"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_processed_hospital_admission_weekly",
						"type": "DatasetReference"
					},
					"name": "WeeklySink"
				},
				{
					"dataset": {
						"referenceName": "ds_hospital_admission_daily",
						"type": "DatasetReference"
					},
					"name": "DailySink"
				}
			],
			"transformations": [
				{
					"name": "SelectReqdFields",
					"description": "Renaming HospitalAdmissionSource to selectReqFields with columns 'country, indicator, date, year_week, value, source, url'"
				},
				{
					"name": "LookupCountry"
				},
				{
					"name": "SplitDailyFromWeekly"
				},
				{
					"name": "DeriveECDCYearWeek"
				},
				{
					"name": "AggDimDate"
				},
				{
					"name": "joinwithdate"
				},
				{
					"name": "PivotWeekly"
				},
				{
					"name": "sortWeekly",
					"description": "Sorting rows on columns 'reported_year_week_country' "
				},
				{
					"name": "selectweekly"
				},
				{
					"name": "PivotDaily"
				},
				{
					"name": "sortDaily"
				},
				{
					"name": "SelectDaily"
				},
				{
					"name": "SelectreqdFields2",
					"description": "Renaming LookupCountry to SelectreqdFields2 with columns 'country, indicator, reported_date, year_week, value, source, "
				}
			],
			"scriptLines": [
				"source(output(",
				"          country as string,",
				"          indicator as string,",
				"          date as date,",
				"          year_week as string,",
				"          value as double,",
				"          source as string,",
				"          url as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> HospitalAdmissionSource",
				"source(output(",
				"          date_key as string,",
				"          date as string,",
				"          year as string,",
				"          month as string,",
				"          day as string,",
				"          day_name as string,",
				"          day_of_year as string,",
				"          week_of_month as string,",
				"          week_of_year as string,",
				"          month_name as string,",
				"          year_month as string,",
				"          year_week as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> DimDataSource",
				"source(output(",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string,",
				"          population as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> CountrySource",
				"HospitalAdmissionSource select(mapColumn(",
				"          country,",
				"          indicator,",
				"          date,",
				"          year_week,",
				"          value,",
				"          source",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectReqdFields",
				"SelectReqdFields, CountrySource lookup(SelectReqdFields@country == CountrySource@country,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> LookupCountry",
				"SelectreqdFields2 split(SelectreqdFields2@country == 'Weekly new hospital admission per 100k' || SelectreqdFields2@country == 'Weekly ICU admission per 100k',",
				"     disjoint: false) ~> SplitDailyFromWeekly@(Weekly, Daily)",
				"DimDataSource derive(ecdc_year_week = year+'-w'+lpad(week_of_year,2,'0')) ~> DeriveECDCYearWeek",
				"DeriveECDCYearWeek aggregate(groupBy(ecdc_year_week),",
				"     week_start_date = min(date),",
				"          week_end_date = max(date)) ~> AggDimDate",
				"SplitDailyFromWeekly@Weekly, AggDimDate join(year_week == ecdc_year_week,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinwithdate",
				"joinwithdate pivot(groupBy(country_code_2_digit,",
				"          country_code_3_digit,",
				"          population,",
				"          year_week,",
				"          source,",
				"          week_start_date,",
				"          week_end_date,",
				"          country),",
				"     pivotBy(indicator, [Weekly new hospital admission per 100k, Weekly ICU admission per 100k]),",
				"     count = sum(value),",
				"     columnNaming: '$N$V',",
				"     lateral: true) ~> PivotWeekly",
				"PivotWeekly sort(desc(year_week, true),",
				"     asc(country, true)) ~> sortWeekly",
				"sortWeekly select(mapColumn(",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          population,",
				"          year_week,",
				"          source,",
				"          week_start_date,",
				"          week_end_date,",
				"          new_hospital_occupancy_count = {countWeekly new hospital admission per 100k},",
				"          new_icu_occupancy_count = {countWeekly ICU admission per 100k},",
				"          country",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> selectweekly",
				"SplitDailyFromWeekly@Daily pivot(groupBy(country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          population,",
				"          reported_date,",
				"          source),",
				"     pivotBy(indicator, [Daily Hospital Occupancy, Daily ICU Occupancy]),",
				"     count = sum(value),",
				"     columnNaming: '$N$V',",
				"     lateral: true) ~> PivotDaily",
				"PivotDaily sort(desc(reported_date, true),",
				"     asc(country, true)) ~> sortDaily",
				"sortDaily select(mapColumn(",
				"          country,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          population,",
				"          reported_date,",
				"          source,",
				"          hospital_occupancy_count = {countDaily Hospital Occupancy},",
				"          icu_occupancy_count = {countDaily ICU Occupancy}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectDaily",
				"LookupCountry select(mapColumn(",
				"          country = indicator,",
				"          date,",
				"          year_week,",
				"          value,",
				"          source,",
				"          country_code_2_digit,",
				"          country_code_3_digit,",
				"          population,",
				"          continent",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectreqdFields2",
				"selectweekly sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          country_code as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> WeeklySink",
				"SelectDaily sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          country_code as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          country as string,",
				"          country_code_2_digit as string,",
				"          country_code_3_digit as string,",
				"          continent as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DailySink"
			]
		}
	}
}